<h1>Listing new_features</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @new_features.each do |new_feature| %>
      <tr>
        <td><%= new_feature.name %></td>
        <td><%= new_feature.content %></td>
        <td><%= link_to 'Show', new_feature %></td>
        <td><%= link_to 'Edit', edit_new_feature_path(new_feature) %></td>
        <td><%= link_to 'Destroy', new_feature, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New New feature', new_new_feature_path %>
